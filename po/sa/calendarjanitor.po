#, kde-format
msgid "--fix is incompatible with --backup"
msgstr "--fix --backup इत्यनेन सह असङ्गतम् अस्ति"

#: main.cpp:109
#, kde-format
msgid "--strip-old-alarms is incompatible with --backup"
msgstr "--strip-old-alarms --backup इत्यनेन सह असङ्गतम् अस्ति"

#: main.cpp:114
#, kde-format
msgid "--strip-old-alarms is incompatible with --fix"
msgstr "--strip-old-alarms इति --fix इत्यनेन सह असङ्गतम् अस्ति"

#: main.cpp:125
#, kde-format
msgid "Running in fix mode."
msgstr "फिक्स मोड् मध्ये चालनम्।"

#: main.cpp:129
#, kde-format
msgid "Please specify a output file."
msgstr "कृपया एकं आउटपुट् सञ्चिकां निर्दिशन्तु ।"

#: main.cpp:134
#, kde-format
msgid "Running in scan only mode."
