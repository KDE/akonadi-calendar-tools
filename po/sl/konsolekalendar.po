msgid "  %1 (like %2, but more compact)"
msgstr "  %1 (kot %2, vendar bolj zgoščeno)"

#: main.cpp:167
#, kde-format
msgctxt "comma-separated values export"
msgid "  %1 (Comma-Separated Values)"
msgstr "  %1 (z vejicami ločene vrednosti)"

